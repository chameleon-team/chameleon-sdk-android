<script cml-type="interface">
    // 定义模块的interface
    interface createStoreInterface {
      createStore(options: CMLObject) : CMLObject;
    }
</script>

<script cml-type="web">
    import createStore from '../../platform/web'

    class Method implements createStoreInterface {
      createStore(options) {
        return createStore(options)
      }
    }
    export default new Method();
</script>

