<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.zkml.care.member">

    <uses-feature android:glEsVersion="0x00020000" />

    <application
        android:name=".MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name_zh"
        tools:replace="android:allowBackup,android:label"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.didi.chameleon.example.MainActivity">
        </activity>
        <activity android:name="cn.zkml.care.member.WeexMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.didi.chameleon.example.CmlWeexViewActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.didi.chameleon.example.CmlWebViewActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>